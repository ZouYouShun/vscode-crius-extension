{
  "name": "crius-tool",
  "displayName": "Crius",
  "description": "crius tool",
  "publisher": "declanzou",
  "version": "0.0.8",
  "icon": "icon.png",
  "repository": "https://github.com/ZouYouShun/vscode-crius-extension",
  "engines": {
    "vscode": "^1.40.0"
  },
  "categories": [
    "Formatters"
  ],
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "crius-tool.criusFormat",
        "title": "Crius: Format example."
      },
      {
        "command": "crius-tool.criusProps",
        "title": "Crius: Create typescript interface with crius example."
      },
      {
        "command": "crius-tool.downloadTestItCase",
        "title": "Crius: Create automation script from TestIt case."
      }
    ],
    "keybindings": [
      {
        "command": "crius-tool.criusFormat",
        "key": "shift+cmd+i s",
        "when": "editorTextFocus"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "crius",
      "properties": {
        "crius-tool.spaceNumber": {
          "type": "integer",
          "default": 4,
          "description": "that number of format begin space",
          "scope": "resource"
        },
        "crius-tool.decoratorSort": {
          "type": "array",
          "description": "sort of that decorator",
          "scope": "resource"
        }
      }
    }
  },
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "rm -rf ./out && npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/vscode": "^1.40.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "eslint": "^6.8.0",
    "@typescript-eslint/parser": "^2.30.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "typescript": "^3.8.3"
  }
}
